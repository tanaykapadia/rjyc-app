<?xml version='1.0' encoding='utf-8'?>
<widget id="com.rjyc.androidapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RJYC</name>
    <description>
        An application for RJYC members to know more about the club.
    </description>
    <author email="tkapadia2911@gmail.com" href="https://github.com/tanaykapadia">
        Tanay Kapadia
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="1.1.0" />
    <access origin="*" />
    <allow-navigation href="*"/>
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="assets/logo.png" />
    </platform>
</widget>